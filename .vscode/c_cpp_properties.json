{
    "configurations": [
        {
            "name": "Original Xbox",
            "includePath": [
                "${NXDK_DIR}/**",
                "${NXDK_DIR}/lib/**",
                "${workspaceFolder}/xbmc/**"
            ],
            "defines": [
                "NXDK",
                "_XBOX",
                "_WIN32",
                "_MSC_VER",
                "HAS_GL"
            ],
            "compilerPath": "/usr/bin/clang",
            "cStandard": "c23",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x64",
            "compilerArgs": [
                "-include",
                "${workspaceFolder}/xbmc/unimplemented.h"
            ]
        }
    ],
    "version": 4
}